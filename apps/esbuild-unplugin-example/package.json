{
  "name": "@stylexswc/esbuild-unplugin-example",
  "description": "A simple esbuild unplugin example to test @stylexswc/unplugin/esbuild",
  "version": "0.14.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "node scripts/build.mjs",
    "clean": "rimraf dist",
    "dev": "node scripts/dev.mjs",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "prebuild": "pnpm run clean",
    "predev": "pnpm run clean",
    "prestart": "pnpm run build",
    "serve": "serve --listen 3002 .",
    "test:visual": "playwright test"
  },
  "dependencies": {
    "@stylexjs/open-props": "^0.11.1",
    "@stylexjs/stylex": "^0.17.4",
    "@stylexswc/design-system": "0.14.0",
    "@stylexswc/rs-compiler": "0.14.0",
    "@stylexswc/unplugin": "0.14.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@stylexswc/playwright": "0.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "esbuild": "^0.27.2",
    "rimraf": "^6.0.1",
    "serve": "^14.2.5"
  }
}
