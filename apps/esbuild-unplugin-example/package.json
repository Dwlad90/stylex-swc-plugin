{
  "name": "@stylexswc/esbuild-unplugin-example",
  "description": "A simple esbuild unplugin example to test @stylexswc/unplugin/esbuild",
  "version": "0.13.0-rc.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "node scripts/build.mjs",
    "clean": "rimraf dist",
    "dev": "node scripts/dev.mjs",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "prebuild": "pnpm run clean",
    "predev": "pnpm run clean",
    "prestart": "pnpm run build",
    "serve": "serve --listen 3002 .",
    "test:visual": "playwright test"
  },
  "dependencies": {
    "@stylexjs/open-props": "^0.11.1",
    "@stylexjs/stylex": "^0.16.3",
    "@stylexswc/design-system": "0.13.0-rc.2",
    "@stylexswc/rs-compiler": "0.13.0-rc.2",
    "@stylexswc/unplugin": "0.13.0-rc.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@stylexswc/playwright": "0.13.0-rc.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "esbuild": "^0.25.10",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4"
  }
}
