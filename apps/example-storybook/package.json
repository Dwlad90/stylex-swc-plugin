{
  "name": "@stylexswc/example-storybook",
  "version": "0.12.1",
  "private": true,
  "scripts": {
    "build": "storybook build",
    "clean": "rimraf storybook-static",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "prebuild": "pnpm run clean",
    "predev": "pnpm run clean",
    "prestart": "pnpm run build",
    "serve": "serve storybook-static -l 3015",
    "storybook": "storybook dev -p 6006 --no-open",
    "test": "vitest run --project=snapshots",
    "test:ui": "vitest --project=snapshots --ui",
    "test:update": "vitest run --project=snapshots -u",
    "test:visual": "playwright test"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-a11y": "^9.1.7",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-links": "^9.1.7",
    "@storybook/addon-vitest": "^9.1.7",
    "@storybook/react": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@stylexjs/eslint-plugin": "^0.16.2",
    "@stylexjs/stylex": "^0.16.2",
    "@stylexswc/playwright": "0.12.1",
    "@stylexswc/postcss-plugin": "0.12.1",
    "@stylexswc/unplugin": "0.12.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-storybook": "^9.1.7",
    "jsdom": "^25.0.1",
    "playwright": "^1.55.1",
    "postcss-nesting": "^13.0.2",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-jsx-remove-attributes": "^3.1.1",
    "rollup-plugin-node-externals": "^8.1.1",
    "serve": "^14.2.4",
    "storybook": "^9.1.7",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
