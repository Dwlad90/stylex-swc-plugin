{
  "name": "@stylexswc/example-storybook",
  "version": "0.14.0",
  "private": true,
  "scripts": {
    "build": "storybook build",
    "clean": "rimraf storybook-static",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "prebuild": "pnpm run clean",
    "predev": "pnpm run clean",
    "prestart": "pnpm run build",
    "serve": "serve storybook-static -l 3015",
    "storybook": "storybook dev -p 6006 --no-open",
    "test": "vitest run --project=snapshots",
    "test:ui": "vitest --project=snapshots --ui",
    "test:update": "vitest run --project=snapshots -u",
    "test:visual": "playwright test"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.7",
    "@storybook/addon-docs": "^10.1.7",
    "@storybook/addon-links": "^10.1.7",
    "@storybook/addon-vitest": "^10.1.7",
    "@storybook/react": "^10.1.7",
    "@storybook/react-vite": "^10.1.11",
    "@stylexjs/eslint-plugin": "^0.17.4",
    "@stylexjs/stylex": "^0.17.4",
    "@stylexswc/playwright": "0.14.0",
    "@stylexswc/postcss-plugin": "0.14.0",
    "@stylexswc/unplugin": "0.14.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/browser": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.6",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-storybook": "^10.1.11",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss-nesting": "^13.0.2",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-jsx-remove-attributes": "^3.1.1",
    "rollup-plugin-node-externals": "^8.1.2",
    "serve": "^14.2.5",
    "storybook": "^10.1.11",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.14"
  }
}
