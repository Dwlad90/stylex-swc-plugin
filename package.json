{
  "name": "@stylexswc/stylexswc",
  "version": "0.6.6",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo clean",
    "dev": "turbo dev",
    "docs": "scripty  --rust",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "turbo run format:check --continue",
    "lint": "turbo lint --continue",
    "lint:check": "turbo run lint:check",
    "prepare": "husky",
    "test": "turbo run test --continue",
    "typecheck": "turbo run typecheck --continue"
  },
  "config": {
    "scripty": {
      "path": "./scripts/packages"
    }
  },
  "dependencies": {
    "scripty": "^2.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.3.0",
    "@stylexswc/eslint-config": "workspace:*",
    "@stylexswc/typescript-config": "workspace:*",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.2",
    "lint-staged": "^15.2.7",
    "prettier": "^3.2.5",
    "shellcheck": "^3.0.0",
    "syncpack": "^12.4.0",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwlad90/stylex-swc-plugin"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "packageManager": "pnpm@9.15.4",
  "pnpm": {
    "overrides": {
      "webpack@<5.0.0": "^5.0.0",
      "@types/webpack@<5.0.0": "^5.0.0",
      "micromatch@<4.0.8": "^4.0.8",
      "cross-spawn@<6.0.6": "^6.0.6",
      "cross-spawn@>=7.0.0 <7.0.5": "^7.0.5",
      "path-to-regexp@<0.1.12": "^0.1.12",
      "nanoid@<3.3.8": "^3.3.8",
      "lexical-core@<1.0.0": "^1.0.0",
      "lexical@<=6.1.1": "^6.1.1",
      "@farmfe/runtime@<=0.12.8": "^0.12.10",
      "koa@>=2.0.0 <2.15.4": "^2.15.4",
      "esbuild@<=0.24.2": "^0.25.0",
      "@babel/core@<7.26.9": "^7.26.9"
    },
    "patchedDependencies": {
      "scripty@2.1.1": "patches/scripty@2.1.1.patch"
    }
  }
}
