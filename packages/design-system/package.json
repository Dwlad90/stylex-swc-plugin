{
  "name": "@stylexswc/design-system",
  "description": "A design system for stylex workspace examples",
  "version": "0.14.1",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./Text": "./src/Text.tsx",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.json"
    ],
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty --ts"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexjs/stylex": "^0.17.4",
    "@stylexswc/rs-compiler": "0.14.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  }
}
