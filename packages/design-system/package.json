{
  "name": "@stylexswc/design-system",
  "description": "A design system for stylex workspace examples",
  "version": "0.7.0-rc.1",
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexjs/stylex": "^0.11.1",
    "@stylexswc/rs-compiler": "0.7.0-rc.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2"
  },
  "exports": {
    ".": "./dist/index.js",
    "./Text": "./dist/Text.js",
    "./tokens.stylex": "./dist/tokens.stylex.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty"
  },
  "sideEffects": false
}
