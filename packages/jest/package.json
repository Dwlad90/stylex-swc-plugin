{
  "name": "@stylexswc/jest",
  "description": "Jest transformer for Stylex SWC",
  "version": "0.14.2",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty --ts"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexswc/rs-compiler": "0.14.2"
  },
  "devDependencies": {
    "@babel/types": "^7.29.0",
    "@jest/transform": "^30.0.5",
    "@jest/types": "^30.2.0",
    "@stylexswc/eslint-config": "0.14.2",
    "@stylexswc/typescript-config": "0.14.2",
    "@types/node": "^25.2.0"
  },
  "keywords": [
    "jest",
    "stylex",
    "swc"
  ],
  "main": "dist/index.js",
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin"
}
