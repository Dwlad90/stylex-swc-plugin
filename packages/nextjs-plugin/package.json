{
  "name": "@toss/stylexswc-nextjs-plugin",
  "description": "StyleX NextJS plugin with NAPI-RS compiler",
  "version": "0.11.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@toss/stylexswc-rs-compiler": "0.11.0",
    "@toss/stylexswc-webpack-plugin": "0.11.0"
  },
  "devDependencies": {
    "@babel/types": "^7.28.2",
    "@toss/stylexswc-eslint-config": "0.11.0",
    "@toss/stylexswc-typescript-config": "0.11.0",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^24.1.0",
    "next": "^15.4.7",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "webpack": "^5.101.0"
  },
  "peerDependencies": {
    "next": ">=15.0.0"
  },
  "keywords": [
    "next",
    "nextjs",
    "nextjs-plugin",
    "stylex",
    "swc"
  ],
  "main": "dist/index.js",
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin"
}
