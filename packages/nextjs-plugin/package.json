{
  "name": "@stylexswc/nextjs-plugin",
  "description": "StyleX NextJS plugin with NAPI-RS compiler",
  "version": "0.8.2-dev.1",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexswc/rs-compiler": "0.8.2-dev.1",
    "@stylexswc/webpack-plugin": "0.8.2-dev.1"
  },
  "devDependencies": {
    "@babel/types": "^7.27.7",
    "@stylexswc/eslint-config": "0.8.2-dev.1",
    "@stylexswc/typescript-config": "0.8.2-dev.1",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^22.15.29",
    "next": "^15.3.4",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "webpack": "^5.99.8"
  },
  "peerDependencies": {
    "next": ">=15.0.0"
  },
  "keywords": [
    "next",
    "nextjs",
    "nextjs-plugin",
    "stylex",
    "swc"
  ],
  "main": "dist/index.js",
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin"
}
