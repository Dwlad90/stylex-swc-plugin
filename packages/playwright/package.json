{
  "name": "@stylexswc/playwright",
  "description": "Playwright for Stylex SWC",
  "version": "0.11.2-rc.2",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty --ts"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@stylexswc/eslint-config": "0.11.2-rc.2",
    "@stylexswc/typescript-config": "0.11.2-rc.2",
    "@types/node": "^24.7.2",
    "playwright": "^1.55.1"
  },
  "keywords": [
    "playwright"
  ],
  "main": "dist/index.js",
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin"
}
