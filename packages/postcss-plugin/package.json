{
  "name": "@stylexswc/postcss-plugin",
  "description": "StyleX PostCSS plugin with NAPI-RS compiler",
  "version": "0.7.0-rc.4",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexjs/babel-plugin": "^0.11.1",
    "@stylexswc/rs-compiler": "0.7.0-rc.4",
    "fast-glob": "^3.3.3",
    "glob-parent": "^6.0.2",
    "is-glob": "^4.0.3",
    "postcss": "^8.5.3"
  },
  "devDependencies": {
    "@babel/types": "^7.26.9",
    "@stylexswc/eslint-config": "0.7.0-rc.4",
    "@stylexswc/typescript-config": "0.7.0-rc.4",
    "@types/babel__core": "^7.20.5",
    "@types/glob-parent": "^5.1.3",
    "@types/is-glob": "^4.0.4",
    "@types/loader-utils": "^2.0.6",
    "@types/node": "^22.13.9"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "stylex",
    "swc"
  ],
  "main": "dist/index.js",
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin"
}
