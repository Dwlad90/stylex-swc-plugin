{
  "name": "@stylexswc/rollup-plugin",
  "description": "StyleX rollup plugin with NAPI-RS compiler",
  "version": "0.6.6",
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexjs/babel-plugin": "^0.11.1",
    "@stylexswc/rs-compiler": "0.6.6",
    "lightningcss": "^1.27.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "browserslist": "^4.24.4",
    "rollup": "^4.25.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__builds__",
      "/__fixtures__/"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "rollup-plugin",
    "rolup",
    "stylex",
    "swc"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin",
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty"
  },
  "sideEffects": false
}
