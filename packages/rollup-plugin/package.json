{
  "name": "@stylexswc/rollup-plugin",
  "description": "StyleX rollup plugin with NAPI-RS compiler",
  "version": "0.11.0-rc.1",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "scripty --ts --flatten",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "jest",
    "typecheck": "scripty"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexjs/babel-plugin": "^0.15.3",
    "@stylexswc/rs-compiler": "0.11.0-rc.1",
    "lightningcss": "^1.30.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-swc": "^0.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "browserslist": "^4.25.1",
    "jest": "^30.0.5",
    "jest-chain-transform": "^0.0.8",
    "rollup": "^4.46.2"
  },
  "keywords": [
    "rollup-plugin",
    "rolup",
    "stylex",
    "swc"
  ],
  "main": "dist/index.js",
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin"
}
