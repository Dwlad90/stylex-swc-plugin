{
  "name": "@stylexswc/swc-plugin",
  "description": "Haaretz theme plugin",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "cargo build --lib --release --target=wasm32-wasi",
    "clean": "del-cli dist",
    "precommit": "lint-staged",
    "prepublishOnly": "cargo build-wasi --release",
    "prepush": "lint-prepush",
    "test": "NODE_ENV=test cargo test --lib --bins --tests"
  },

  "keywords": [
    "swc-plugin"
  ],
  "main": "target/wasm32-wasi/release/stylex_swc_plugin.wasm",
  "preferUnplugged": true
}
