{
  "name": "@stylexswc/turbopack-plugin",
  "description": "StyleX turbopack loader with NAPI-RS compiler",
  "version": "0.12.1",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./loader": {
      "import": "./dist/turbopack-loader.js",
      "require": "./dist/turbopack-loader.js",
      "types": "./dist/turbopack-loader.d.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "scripty --ts",
    "check:artifacts": "scripty",
    "clean": "del-cli dist",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "postbuild": "pnpm run check:artifacts",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "prepush": "lint-prepush",
    "test": "echo \"Error: no test specified\" && exit 0",
    "typecheck": "scripty --ts"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  },
  "dependencies": {
    "@stylexswc/rs-compiler": "0.12.1",
    "loader-utils": "^3.3.1"
  },
  "devDependencies": {
    "webpack": "^5.102.1",
    "@babel/types": "^7.28.4",
    "@stylexswc/eslint-config": "0.12.1",
    "@stylexswc/typescript-config": "0.12.1",
    "@types/babel__core": "^7.20.5",
    "@types/loader-utils": "^3.0.0",
    "@types/node": "^24.7.2"
  },
  "keywords": [
    "stylex",
    "swc",
    "turbopack",
    "turbopack-plugin"
  ],
  "main": "dist/index.js",
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin"
}