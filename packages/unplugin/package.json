{
  "name": "@stylexswc/unplugin",
  "type": "module",
  "version": "0.7.0-dev.1",
  "packageManager": "pnpm@9.15.4",
  "description": "Unplugin for StyleX RS compiler",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/Dwlad90/stylex-swc-plugin/tree/develop/packages/unplugin#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": "https://github.com/Dwlad90/stylex-swc-plugin",
  "bugs": {
    "url": "https://github.com/Dwlad90/stylex-swc-plugin/issues"
  },
  "sideEffects": false,
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "stylex",
    "farm",
    "esbuild",
    "rspack",
    "rsbuild",
    "swc"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "check:artifacts": "scripty",
    "lint": "eslint . --color",
    "lint:check": "eslint . --color --format json --output-file dist/eslint_report.json",
    "play": "pnpm -C playground run dev",
    "postbuild": "pnpm run check:artifacts",
    "prepublishOnly": "pnpm run build",
    "start": "esno src/index.ts",
    "test": "vitest  --run",
    "test:watch": "vitest",
    "test:update": "vitest -u",
    "test:cov": "vitest --coverage",
    "typecheck": "scripty"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1.6.6",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@stylexjs/babel-plugin": "^0.11.1",
    "@stylexswc/rs-compiler": "0.7.0-dev.1",
    "unplugin": "^2.2.0",
    "vite": "^6.2.1",
    "vite-plugin-inspect": "^11.0.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.16.0",
    "@nuxt/schema": "^3.16.0",
    "@types/node": "^22.13.9",
    "chalk": "^5.4.1",
    "eslint": "^9.21.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.3",
    "nodemon": "^3.1.9",
    "rollup": "^4.34.9",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vitest": "^3.0.8",
    "webpack": "^5.98.0"
  },
  "config": {
    "scripty": {
      "path": "../../scripts/packages"
    }
  }
}
