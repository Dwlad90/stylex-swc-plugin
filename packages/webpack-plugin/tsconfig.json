{
  "extends": "@toss/stylexswc-typescript-config/base.json",
  "compilerOptions": {
    "outDir": "dist",
    "moduleResolution": "Node",
    "module": "CommonJS",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}