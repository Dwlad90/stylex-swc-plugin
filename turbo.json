{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".eslintrc.js",
    ".prettierrc.js",
    "package.json",
    "Cargo.toml",
    "clippy.toml",
    "turbo.json",
    "typings/**"
  ],
  "globalEnv": [
    "NODE_ENV",
    "STYLEX_DEBUG",
    "PORT"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "clean"
      ],
      "outputs": [
        "dist/**/*",
        "npm/**/*",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "lint:check": {
      "outputs": [
        "dist/eslint_report.json"
      ]
    },
    "@toss/stylexswc-rs-compiler#lint:check": {
      "dependsOn": [
        "^build",
        "build",
        "clean"
      ]
    },
    "@toss/stylexswc-path-resolver#lint:check": {
      "dependsOn": [
        "^build",
        "build",
        "clean"
      ]
    },
    "@toss/stylexswc-shared#lint:check": {
      "dependsOn": [
        "^build",
        "build",
        "clean"
      ]
    },
    "@toss/stylexswc-test-parser#lint:check": {
      "dependsOn": [
        "^build",
        "build",
        "clean"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": [
        "^build",
        "clean"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build",
        "clean"
      ],
      "outputs": [
        "**/__swc_snapshots__",
        "**/__snapshots__"
      ]
    },
    "format:check": {},
    "bench": {
      "dependsOn": [
        "^build",
        "build",
        "clean"
      ],
      "cache": false
    },
    "test:visual": {
      "dependsOn": [
        "^build",
        "build",
        "clean"
      ],
      "outputs": [
        "**/.playwright-snapshots"
      ]
    }
  }
}